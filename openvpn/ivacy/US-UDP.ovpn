client

dev tun
remote usil1-ovpn-udp.ivacy.net 53
proto udp
nobind
persist-key
persist-tun
tls-auth /etc/openvpn/ivacy/Wdc.key 1
ca /etc/openvpn/ivacy/ca.crt
cipher AES-256-CBC
comp-lzo
verb 1
mute 20
float
route-method exe
route-delay 2
auth-user-pass /config/openvpn-credentials.txt
auth-retry interact
explicit-exit-notify 2
ifconfig-nowarn
auth-nocache 