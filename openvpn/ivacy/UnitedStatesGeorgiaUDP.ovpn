client
dev tun
proto udp
remote usga2-ovpn-udp.dns2use.com 53
nobind
persist-key
persist-tun
ca /etc/openvpn/ivacy/ca.crt
tls-auth /etc/openvpn/ivacy/Wdc.key 1
cipher AES-256-CBC
comp-lzo
verb 1
mute 20
route-method exe
route-delay 2
route 0.0.0.0 0.0.0.0
float
auth-user-pass /config/openvpn-credentials.txt
auth-retry interact
explicit-exit-notify 2
ifconfig-nowarn
auth-nocache
