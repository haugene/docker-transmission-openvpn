client
proto tcp
dev tun0
remote 127.0.0.1 45453
tun0-mtu 1500
route 103.9.162.253 255.255.255.255 net_gateway
resolv-retry infinite
nobind
persist-key
persist-tun
ns-cert-type server
ca /etc/openvpn/vpnarea/ca.crt
cipher AES-256-CBC
auth SHA256
comp-lzo
verb 3
auth-user-pass /config/openvpn-credentials.txt
mute-replay-warnings
passtos
tun-mtu 1500
