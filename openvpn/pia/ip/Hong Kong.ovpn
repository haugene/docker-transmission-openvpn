client
dev tun
proto udp
remote 119.81.135.47 1198
resolv-retry infinite
nobind
persist-key
persist-tun
cipher aes-128-cbc
auth sha1
tls-client
remote-cert-tls server

auth-user-pass /config/openvpn-credentials.txt
comp-lzo
verb 1
reneg-sec 0
crl-verify /etc/openvpn/pia/ip/crl.rsa.2048.pem
ca /etc/openvpn/pia/ip/ca.rsa.2048.crt
disable-occ
