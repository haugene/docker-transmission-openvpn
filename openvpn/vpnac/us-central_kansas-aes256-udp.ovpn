remote-random
remote us6.vpn.ac 8000
remote us6.vpn.ac 50000
dev tun
tls-client
persist-tun
persist-key
nobind
pull
redirect-gateway def1
route-delay 3
auth-user-pass /config/openvpn-credentials.txt
verb 3
explicit-exit-notify 2
remote-cert-tls server
