remote-random
remote ca1.vpn.ac 12200
remote ca1.vpn.ac 26000
remote ca1.vpn.ac 88
;comment the 3 options above and leave only port 53 for better firewall by-passing support
remote ca1.vpn.ac 53
proto udp
dev tun
tls-client
persist-key
persist-tun
nobind
pull
redirect-gateway def1
route-delay 3
verb 3
remote-cert-tls server
auth SHA256
auth-user-pass /config/openvpn-credentials.txt
cipher AES-128-CBC
mute-replay-warnings
key-direction 1
explicit-exit-notify 2
comp-lzo
persist-remote-ip
