auth SHA512
#auth-nocache

resolv-retry infinite
nobind
persist-key
client
verb 3
auth-user-pass /config/openvpn-credentials.txt

verify-x509-name vpn.trust.zone name

ping 3
ping-restart 10

#remote-cert-tls server
#remote-cert-ku f6

