client
dev tun
proto udp
remote 198.50.152.89 1194
resolv-retry infinite
nobind
tun-mtu 1500
tun-mtu-extra 32
mssfix 1450
persist-key
persist-tun
ping 15
ping-restart 0
ping-timer-rem
reneg-sec 0

remote-cert-tls server

#mute 10000
# Read authentication persisted from env
auth-user-pass /config/openvpn-credentials.txt

comp-lzo
verb 3
pull
fast-io
cipher AES-256-CBC

