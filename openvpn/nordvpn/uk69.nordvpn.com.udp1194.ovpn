ping 15
ping-restart 0
ping-timer-rem
reneg-sec 0
explicit-exit-notify 3

remote-cert-tls server

#mute 10000
auth-user-pass /config/openvpn-credentials.txt

comp-lzo
verb 3
pull
fast-io
cipher AES-256-CBC

