# Njalla VPN Configuration
client
dev tun
proto udp
remote openvpn.njalla.no 1194
remote openvpn.njalla.in 1194
remote openvpn.njalla.fo 1194
resolv-retry infinite
nobind

key-direction 1

#ca [inline]
#tls-client
#tls-auth [inline] 1
#cert [inline]
#key [inline]

remote-cert-tls server
auth-nocache
#compress lzo
#comp-lzo

keepalive 10 30
cipher AES-256-CBC

persist-key
persist-tun

tun-mtu 1500
mssfix 1200
replay-window 512 60

# Enable this if your system does support it!
tls-version-min 1.2

# Wireless networks often produce a lot
# of duplicate packets.  Set this flag
# to silence duplicate packet warnings.
mute-replay-warnings


verb 0

