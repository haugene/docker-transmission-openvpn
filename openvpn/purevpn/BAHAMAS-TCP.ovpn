client
dev tun0
proto tcp
remote bs1-ovpn-tcp.pointtoserver.com 80
persist-key
persist-tun
ca /etc/openvpn/purevpn/ca.crt
tls-auth /etc/openvpn/purevpn/Wdc.key 1
cipher AES-256-CBC
comp-lzo
verb 1
mute 20
route-method exe
route-delay 2
route 0.0.0.0 0.0.0.0
float
auth-user-pass /config/openvpn-credentials.txt
auth-retry interact
ifconfig-nowarn

